#!/bin/bash

if [ "${1}" = "" ] || [ "${2}" = "" ] || [ "${3}" = "" ] || [ "${4}" = "" ]; then
    echo "4 args are required."
    exit
fi

. $(dirname $0)/../../conf/shell/profile
. $(dirname $0)/../../conf/shell/rc

set-profile
echo
set-bashrc
echo
set-gitconfig ${1} ${2}
echo
set-smb ${3}
echo
set-raspbian-wifi ${4}
