. ${HOME}/opt/rtools/bin/detect-os > /dev/null

case $(hostname) in
    host1)
        case ${os_version} in
            *)
                ;;
        esac
        ;;
    *)
        ;;
esac

# Zsh
case $TERM in
    xterm*)
        bindkey '^[[H' beginning-of-line
        bindkey '^[[F' end-of-line
        ;;
esac
unset LESS

# Rust
if [ -d "$HOME/.cargo" ]; then
    . "$HOME/.cargo/env"
fi

export _ZSHRC_LOADED_=1
