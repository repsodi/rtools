. ${HOME}/opt/rtools/bin/detect-os > /dev/null

case ${os_version} in
    *)
        case $(hostname) in
            *)
                ;;
        esac
        ;;
esac

export PATH=~/opt/rtools/bin/${os_name}:~/opt/rtools/bin:$PATH

# Rust
if [ -d "$HOME/.cargo" ]; then
    . "$HOME/.cargo/env"
fi

export _LOADED_PROFILE_=1
