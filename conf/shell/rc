. ${HOME}/opt/rtools/bin/base.conf > /dev/null

if [ "${_LOADED_PROFILE} != "1" ]; then
    . ${HOME}/opt/rtools/conf/shell/profile
fi

case ${os_conf} in
    *)
        ;;
esac

case $(hostname) in
    *)
        ;;
esac

if [ "${shell_name}" = "zsh" ]; then
    # Oh-My-Zsh
    unset PAGER
    unset LESS

    export _LOADED_RC_ZSH=1
fi

# Rust
if [ -d "$HOME/.cargo" ]; then
    . "$HOME/.cargo/env"
fi

export _LOADED_RC=1
